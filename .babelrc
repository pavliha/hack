{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    ["babel-root-import", {
      "rootPathSuffix": "resources/app"
    }],
    "transform-flow-strip-types",
    "transform-runtime",
    "transform-async-to-generator",
    "transform-class-properties",
    "transform-decorators-legacy"
  ]
}